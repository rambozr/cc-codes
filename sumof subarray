#include <iostream>
using namespace std;

int maxSubArrayBruteForce(int arr[], int n) {
    int maxSum = arr[0];

    // Consider all subarrays
    for (int i = 0; i < n; i++) {
        int currSum = 0;
        for (int j = i; j < n; j++) {
            currSum += arr[j];           // sum of subarray from i to j
            if (currSum > maxSum) {
                maxSum = currSum;        // update maximum
            }
        }
    }

    return maxSum;
}

int main() {
    int arr[] = {-2, 1, -3, 4, -1, 2, 1, -5, 4};
    int n = sizeof(arr) / sizeof(arr[0]);

    cout << "Maximum Subarray Sum (Brute Force) = " 
         << maxSubArrayBruteForce(arr, n) << endl;

    return 0;
}
