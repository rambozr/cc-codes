
#include <iostream>
using namespace std;
int main() {
    int arr[] = {2, 7, 11, 15};
    int n = sizeof(arr) / sizeof(arr[0]);
    int target = 9;

    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (arr[i] + arr[j] == target) {
                cout << "Indices: " << i << " and " << j << endl;
                return 0; // stop after finding one solution
            }
        }
    }
    cout << "No two sum solution found." << endl;
    return 0;
}
