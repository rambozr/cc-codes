#include <iostream>
using namespace std;

string simplifyPath(string path) {
    string res = "/";
    int n = path.size();
    int i = 0;

    while (i < n) {
        while (i < n && path[i] == '/') i++;

        string dir = "";
        while (i < n && path[i] != '/') {
            dir += path[i];
            i++;
        }

        if (dir == "" || dir == ".") continue;
        if (dir == "..") {
            if (res != "/") {
                res.pop_back();
                while (!res.empty() && res.back() != '/') res.pop_back();
                if (res.empty()) res = "/";
            }
        } else {
            if (res.back() != '/') res += "/";
            res += dir;
        }
    }

    return res;
}

int main() {
    string path = "/home/../usr//bin/./";
    cout << simplifyPath(path) << endl;
    return 0;
}
